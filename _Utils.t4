<#+
string Register(string name)
{
	return (name=="SP" || IsIndexRegister(name) ? "Registers." : "MainRegisters.") + name;
}

string TestRegister(string name)
{
	return (name=="SP" || IsIndexRegister(name) ? "Registers." : "Registers.Main.") + name;
}

bool IsIndexRegister(string name)
{
	return (name.StartsWith("IX") || name.StartsWith("IY"));
}
#>